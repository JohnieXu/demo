4BAAWH,GAAX;AACD,CAfM;AAiBP;;;;;;;;;;;;;;;;;;AAMA,OAAO,IAAMM,SAAS,GAAG,SAAZA,SAAY,CAACC,IAAD,EAAOC,IAAP,EAAaC,SAAb,EAA2B;AAClD,MAAI,CAACX,KAAK,CAACC,OAAN,CAAcQ,IAAd,CAAL,EAA0B;AACxBZ,IAAAA,cAAc,CAAC,gCAAD,CAAd;AACD;;AACD,MAAI,CAACG,KAAK,CAACC,OAAN,CAAcS,IAAd,CAAL,EAA0B;AACxBb,IAAAA,cAAc,CAAC,gCAAD,CAAd;AACD;;AACD,MAAI,OAAOc,SAAP,KAAqB,WAArB,IAAoC,OAAOA,SAAP,KAAqB,UAA7D,EAAyE;AACvEd,IAAAA,cAAc,CAAC,kDAAD,CAAd;AACD;;AACD,MAAMe,cAAc,GAAGH,IAAI,CAACI,MAAL,CAAY,UAACC,KAAD,EAAW;AAC5C,WAAOJ,IAAI,CAACK,IAAL,CAAU,UAACC,KAAD,EAAW;AAC1B,UAAI,OAAOL,SAAP,KAAqB,UAAzB,EAAqC;AACnC,eAAOM,OAAO,CAACN,SAAS,CAACG,KAAD,EAAQE,KAAR,CAAV,CAAd;AACD;;AACD,aAAOA,KAAK,KAAKF,KAAjB;AACD,KALM,CAAP;AAMD,GAPsB,CAAvB;AAQA,SAAOF,cAAP;AACD,CAnBM;AAqBP;;;;;;;;;;;;;;;;;;AAMA,OAAO,IAAMM,IAAI,GAAG,SAAPA,IAAO,CAACT,IAAD,EAAOC,IAAP,EAAaC,SAAb,EAA2B;AAC7C,MAAI,CAACX,KAAK,CAACC,OAAN,CAAcQ,IAAd,CAAL,EAA0B;AACxBZ,IAAAA,cAAc,CAAC,gCAAD,CAAd;AACD;;AACD,MAAI,CAACG,KAAK,CAACC,OAAN,CAAcS,IAAd,CAAL,EAA0B;AACxBb,IAAAA,cAAc,CAAC,gCAAD,CAAd;AACD;;AACD,MAAI,OAAOc,SAAP,KAAqB,WAArB,IAAoC,OAAOA,SAAP,KAAqB,UAA7D,EAAyE;AACvEd,IAAAA,cAAc,CAAC,kDAAD,CAAd;AACD;;AACD,MAAMsB,SAAS,GAAGV,IAAI,CAACI,MAAL,CAAY,UAACC,KAAD,EAAW;AACvC,WAAOJ,IAAI,CAACU,KAAL,CAAW,UAACJ,KAAD,EAAW;AAC3B,UAAI,OAAOL,SAAP,KAAqB,UAAzB,EAAqC;AACnC,eAAOM,OAAO,CAACN,SAAS,CAACG,KAAD,EAAQE,KAAR,CAAV,CAAd;AACD;;AACD,aAAOA,KAAK,KAAKF,KAAjB;AACD,KALM,CAAP;AAMD,GAPiB,CAAlB;AAQA,SAAOJ,IAAI,CAACW,MAAL,CAAYF,SAAZ,CAAP;AACD,CAnBM;;;;;;;;;;;;;;;;;;;;;;0BAlDMrB,O;0BAuBAU,S;0BA2BAU,I","sourcesContent":["import { handleArgError } from './utils'\n\n/**\n * 扁平化数组\n * @param {Array} arr 需要扁平化的数组\n */\nexport const flatten = (arr) => {\n  if (!Array.isArray(arr)) {\n    return handleArgError('argument `arr` must be an array')\n  }\n  const set = new Set()\n  const flat = (item) => {\n    if (typeof item === 'undefined') { return }\n    if (!Array.isArray(item)) {\n      set.add(item)\n    } else {\n      item.forEach(item => flat(item))\n    }\n  }\n  arr.forEach(item => flat(item))\n  return [...set]\n}\n\n/**\n * 取两数组的交集\n * @param {Array} arr1 数组1\n * @param {Array} arr2 数组2\n * @param {*} compareFn 比较函数 默认：===\n */\nexport const intersect = (arr1, arr2, compareFn) => {\n  if (!Array.isArray(arr1)) {\n    handleArgError('argument arr1 must be an Array')\n  }\n  if (!Array.isArray(arr2)) {\n    handleArgError('argument arr2 must be an Array')\n  }\n  if (typeof compareFn !== 'undefined' && typeof compareFn !== 'function') {\n    handleArgError('argument compareFn must be undefined or Function')\n  }\n  const intersectedArr = arr1.filter((item1) => {\n    return arr2.some((item2) => {\n      if (typeof compareFn === 'function') {\n        return Boolean(compareFn(item1, item2))\n      }\n      return item2 === item1\n    })\n  })\n  return intersectedArr\n}\n\n/**\n * 取两数组的并集\n * @param {Array} arr1 数组1\n * @param {Array} arr2 数组2\n * @param {*} compareFn 比较函数 默认：===\n */\nexport const join = (arr1, arr2, compareFn) => {\n  if (!Array.isArray(arr1)) {\n    handleArgError('argument arr1 must be an Array')\n  }\n  if (!Array.isArray(arr2)) {\n    handleArgError('argument arr2 must be an Array')\n  }\n  if (typeof compareFn !== 'undefined' && typeof compareFn !== 'function') {\n    handleArgError('argument compareFn must be undefined or Function')\n  }\n  const uniqueArr = arr1.filter((item1) => {\n    return arr2.every((item2) => {\n      if (typeof compareFn === 'function') {\n        return Boolean(compareFn(item1, item2))\n      }\n      return item2 !== item1\n    })\n  })\n  return arr2.concat(uniqueArr)\n}"]}]}