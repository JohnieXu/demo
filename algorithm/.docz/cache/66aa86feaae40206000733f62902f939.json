st debounce = (fn, delay = 300) => {\n  let timer // 当前延时的定时器\n  return (...props) => {\n    timer && clearTimeout(timer)\n    timer = setTimeout(function timeout() {\n      typeof fn === 'function' && fn.call(this, ...props)\n    }, delay);\n  }\n}\n\n// 浅复制\nexport const shallowClone = (obj) => {\n  // number string boolean null undefined\n}\n\n// 深复制\nexport const deepClone = (obj) => {}\n\nexport const delay = (time, isDecorator = false, fn, ...props) => {\n  if (typeof time !== 'number') {\n    handleArgError('argument time must be Number')\n  }\n  console.log(time, isDecorator, fn, [...props])\n  return new Promise((resolve, reject) => {\n    setTimeout(() => {\n      resolve()\n      fn.call(this)\n    }, time)\n  })\n  // return () => {}\n}\n\n// 倒计时\nexport const countDown = () => {}\n"]}]}